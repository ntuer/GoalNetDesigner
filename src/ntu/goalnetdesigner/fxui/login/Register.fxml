<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import java.net.*?>
<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<GridPane alignment="center" hgap="10" vgap="10" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="ntu.goalnetdesigner.fxcontrol.login.RegisterController">
	<padding>
		<Insets bottom="10" left="25" right="25" top="25" />
	</padding>
   <children>
   	<Text text="Register" GridPane.columnIndex="0" GridPane.columnSpan="2" GridPane.rowIndex="0" />
   
   	<Label text="Password:" GridPane.rowIndex="3" />
   
   	<PasswordField fx:id="passwordField" GridPane.columnIndex="1" GridPane.rowIndex="3" />
   
   	<Label text="Confirm Password" GridPane.rowIndex="4" />
   
   	<PasswordField fx:id="confirmPasswordField" GridPane.columnIndex="1" GridPane.rowIndex="4" />
   	<HBox alignment="CENTER" spacing="10" GridPane.columnSpan="2" GridPane.rowIndex="11">
      <children>
            <Button mnemonicParsing="false" onAction="#registerClicked" text="Register" />
            <Button mnemonicParsing="false" onAction="#cancelClicked" text="Cancel" />
      </children>
   	</HBox>
   
   	<Text fx:id="actiontarget" wrappingWidth="98.0" GridPane.rowIndex="10" />
      <Label text="User Name:" GridPane.rowIndex="2" />
      <TextField fx:id="usernameField" GridPane.columnIndex="1" GridPane.rowIndex="2" />
      <Label text="Email:" GridPane.rowIndex="5" />
      <TextField fx:id="emailField" GridPane.columnIndex="1" GridPane.rowIndex="5" />
      <TextField fx:id="questionField" GridPane.columnIndex="1" GridPane.rowIndex="6" />
      <TextField fx:id="answerField" GridPane.columnIndex="1" GridPane.rowIndex="7" />
      <Label text="Security Question:" GridPane.rowIndex="6" />
      <Label text="Answer:" GridPane.rowIndex="7" />
      <Label text="Age:" GridPane.rowIndex="8" />
      <Label text="Education Level" GridPane.rowIndex="9" />
      <TextField fx:id="ageField" GridPane.columnIndex="1" GridPane.rowIndex="8" />
      <ComboBox fx:id="educationComboBox" prefWidth="150.0" GridPane.columnIndex="1" GridPane.rowIndex="9" />
   </children>
   <columnConstraints>
      <ColumnConstraints />
      <ColumnConstraints />
   </columnConstraints>
   <rowConstraints>
      <RowConstraints />
      <RowConstraints />
      <RowConstraints />
      <RowConstraints />
      <RowConstraints />
      <RowConstraints />
      <RowConstraints />
      <RowConstraints />
      <RowConstraints minHeight="10.0" />
      <RowConstraints minHeight="10.0" />
      <RowConstraints />
      <RowConstraints />
   </rowConstraints>
</GridPane>
