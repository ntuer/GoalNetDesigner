<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import java.lang.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.layout.GridPane?>

<GridPane xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="ntu.goalnetdesigner.fxcontrol.ManageTaskFunctionController">
   <rowConstraints>
      <RowConstraints minHeight="10.0" prefHeight="250.0" />
   </rowConstraints>
   <columnConstraints>
      <ColumnConstraints minWidth="150.0" prefWidth="150.0" />
      <ColumnConstraints minWidth="10.0" prefWidth="100.0" />
   </columnConstraints>
   <children>
      <ListView fx:id="functionList" prefHeight="121.0" prefWidth="100.0" />
      <VBox prefHeight="200.0" prefWidth="100.0" spacing="10.0" GridPane.columnIndex="1">
         <children>
            <Button mnemonicParsing="false" onAction="#addButtonOnClick" prefHeight="25.0" prefWidth="92.0" text="Add" />
            <Button mnemonicParsing="false" onAction="#moveUpOnClick" prefHeight="25.0" prefWidth="101.0" text="Move up" />
            <Button mnemonicParsing="false" onAction="#moveDownOnClick" prefHeight="25.0" prefWidth="105.0" text="Move down" />
            <Button mnemonicParsing="false" onAction="#deleteOnClick" prefHeight="25.0" prefWidth="121.0" text="Delete" />
            <Button mnemonicParsing="false" onAction="#saveButtonOnClick" prefHeight="25.0" prefWidth="138.0" text="Save" />
         </children>
         <padding>
            <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
         </padding>
      </VBox>
   </children>
</GridPane>
