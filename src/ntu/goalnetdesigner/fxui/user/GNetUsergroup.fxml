<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import java.lang.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.layout.GridPane?>

<GridPane xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="ntu.goalnetdesigner.fxcontrol.user.GNetUsergroupController">
   <columnConstraints>
      <ColumnConstraints />
   </columnConstraints>
   <rowConstraints>
      <RowConstraints />
      <RowConstraints />
   </rowConstraints>
   <children>
      <AnchorPane prefHeight="236.0" prefWidth="521.0" GridPane.rowIndex="1">
         <children>
            <Label layoutX="14.0" layoutY="14.0" text="Select from the following Goal Net:" />
            <TableView fx:id="usergroupTable" editable="true" layoutX="14.0" layoutY="31.0" prefHeight="160.0" prefWidth="493.0">
              <columns>
                <TableColumn fx:id="nameColumn" prefWidth="134.0" text="Name" />
                <TableColumn fx:id="descriptionColumn" minWidth="0.0" prefWidth="218.0" text="Description" />
                <TableColumn fx:id="canReadColumn" minWidth="0.0" prefWidth="70.0" text="Read" />
                <TableColumn fx:id="canWriteColumn" minWidth="0.0" prefWidth="70.0" text="Write" />
              </columns>
            </TableView>
            <Button fx:id="OKButton" layoutX="237.0" layoutY="197.0" mnemonicParsing="false" onAction="#OKButtonClicked" text="OK" />
         </children>
      </AnchorPane>
   </children>
</GridPane>
