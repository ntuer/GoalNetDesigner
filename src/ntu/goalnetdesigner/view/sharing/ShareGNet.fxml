<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import java.lang.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.layout.GridPane?>

<GridPane xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="ntu.goalnetdesigner.viewcontroller.sharing.ShareGNetController">
   <columnConstraints>
      <ColumnConstraints />
   </columnConstraints>
   <rowConstraints>
      <RowConstraints />
      <RowConstraints />
   </rowConstraints>
   <children>
      <AnchorPane prefHeight="236.0" prefWidth="342.0" GridPane.rowIndex="1">
         <children>
            <Label layoutX="14.0" layoutY="14.0" text="Select from the following Goal Net:" />
            <TableView fx:id="userTable" editable="true" layoutX="14.0" layoutY="31.0" prefHeight="160.0" prefWidth="318.0">
              <columns>
                <TableColumn fx:id="nameColumn" prefWidth="134.0" text="Name" />
                <TableColumn fx:id="accessLevelColumn" minWidth="0.0" prefWidth="183.0" text="Access" />
              </columns>
            </TableView>
            <HBox alignment="CENTER" layoutY="197.0" prefHeight="25.0" prefWidth="342.0" spacing="10.0">
               <children>
                  <Button fx:id="addButton" mnemonicParsing="false" onAction="#addButtonClicked" text="Add" disable="true"/>
                  <Button fx:id="removeButton" mnemonicParsing="false" onAction="#removeButtonClicked" text="Remove" disable="true"/>
               </children>
            </HBox>
         </children>
      </AnchorPane>
   </children>
</GridPane>
