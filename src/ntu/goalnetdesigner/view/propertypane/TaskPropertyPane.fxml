<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import java.lang.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.layout.GridPane?>

<GridPane hgap="10.0" vgap="10.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="ntu.goalnetdesigner.viewcontroller.propertypanecontrol.TaskPropertyPaneController">
   <columnConstraints>
      <ColumnConstraints />
      <ColumnConstraints />
   </columnConstraints>
   <rowConstraints>
      <RowConstraints />
      <RowConstraints />
      <RowConstraints />
      <RowConstraints />
      <RowConstraints />
      <RowConstraints />
   </rowConstraints>
   <children>
      <Label prefHeight="17.0" prefWidth="48.0" text="ID" />
      <TextField fx:id="id" editable="false" GridPane.columnIndex="1" />
      <Label text="Description" GridPane.rowIndex="2" />
      <TextArea fx:id="description" prefHeight="55.0" prefWidth="200.0" GridPane.columnIndex="1" GridPane.rowIndex="2" />
      <TextField fx:id="name" GridPane.columnIndex="1" GridPane.rowIndex="1" />
      <TextField fx:id="cost" GridPane.columnIndex="1" GridPane.rowIndex="3" />
      <TextField fx:id="achievement" GridPane.columnIndex="1" GridPane.rowIndex="4" />
      <Label text="Name" GridPane.rowIndex="1" />
      <Label text="Cost" GridPane.rowIndex="3" />
      <Label text="Achievement" GridPane.rowIndex="4" />
      <Label text="Functions" GridPane.rowIndex="5" />
      <Button mnemonicParsing="false" onAction="#manageFunctionsButtonOnClick" text="Manage Functions" GridPane.columnIndex="1" GridPane.rowIndex="5" />
   </children>
   <padding>
      <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
   </padding>
</GridPane>
